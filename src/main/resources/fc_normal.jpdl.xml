<?xml version="1.0" encoding="UTF-8"?>

<process-definition  xmlns=""  name="fc_normal">


	<start-state name="start-state1">
		<transition to="task-node2"></transition>
	</start-state>


	<task-node name="task-node2">
		<task name="accpt">
			<description>
				#{startData.description}
			</description>
			<assignment actor-id="#{loginEmployee.id}"></assignment>
		</task>
		<transition to="task-node1"></transition>
	</task-node>

	<task-node name="task-node1">
		<task name="operation">
			<assignment pooled-actors="system.manager,system.config"></assignment>
		</task>
		<transition to="end-state1"></transition>
	</task-node>


	<end-state name="end-state1"></end-state>


</process-definition>