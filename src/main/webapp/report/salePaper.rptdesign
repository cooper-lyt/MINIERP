            <method name="open"><![CDATA[

it=orderItems.iterator();
]]></method>
            <method name="fetch"><![CDATA[if (it.hasNext()){
	item = it.next();

	itemTitle = item.getStoreRes().getRes().getName() + " ";
	
	

		storeRes = item.getStoreRes();
		formats = storeRes.getFormatList(); 
		for(i=0;i++;i<formats.size() -1){
			format = formats.get(i);
			itemTitle += format.getFormatDefine().getName() + ":";
			if (format.getFormatDefine().getDataType().equals(FormatDefine.FormatType.WORD)){
				itemTitle += dictionary.getWordValue(format.getFormatValue());
			}else{
				itemTitle += format.getFormatValue();
			}						
		}
		if (item.getStoreRes().getRes().getUnitGroup().getType().equals(UnitGroup.UnitGroupType.FLOAT_CONVERT)){
			
			itemTitle +=  DataFormat.format(storeRes.getFloatConversionRate(),storeRes.getRes().getUnitGroup().getFloatConvertRateFormat());
			itemTitle += item.getStoreRes().getRes().getUnitGroup().getName();
		}
		
	
	
	row['NAME'] = itemTitle;
    


   	row['UNIT'] = item.getResUnit().getName();
    row['COUNT'] = DataFormat.format(item.getUseUnitCount(),item.getResUnit().getCountFormate());
    row['TOTAL_PRICE'] = item.getTotalMoney();
    row['PRICE'] = item.getMoney();
    row['REBATE'] = item.getRebate();
  
    
    
    
    row['ITEM_INFO'] = itemTitle + "  " + row['COUNT']  + "  " + row['UNIT'];

	return true;
}else{


	return false;
}]]></method>
