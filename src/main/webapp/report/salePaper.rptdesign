    <method name="initialize"><![CDATA[
importPackage(Packages.java.lang);
importPackage(Packages.org.jboss.seam);  
importPackage(Packages.com.dgsoft.erp.model); 
importPackage(Packages.com.dgsoft.common);
importPackage(Packages.java.math); 
dictionary = Component.getInstance("dictionary", false);
messages = Component.getInstance("org.jboss.seam.international.messages", false);
	orderHome = Component.getInstance("orderHome",false);
	if (orderHome == null){
		orderHome = Component.getInstance("orderHome",true);
		orderHome.setId(params["orderId"].value);
	}
	orderItems = Component.getInstance("orderHome",false).getInstance().getAllOrderItemList();
	salerName = dictionary.getEmpNameById(orderHome.getInstance().getOrderEmp());
	saleDate = orderHome.getInstance().getCreateDate();
	orderNumber = orderHome.getInstance().getId();
	saleAreaName = orderHome.getInstance().getCustomer().getCustomerArea().getName();
	customerName = orderHome.getInstance().getCustomer().getName();
	orderRebate = orderHome.getInstance().getTotalRebate();
	orderMoney= orderHome.getInstance().getMoney();
	orderRebateMoney = orderHome.getInstance().getResMoney().subtract(orderHome.getInstance().getTotalRebateMoney())
	
	
	if (orderHome.getInstance().isMoneyComplete()){
		orderState = messages.get("order_field_moneyComplete_true"); 
	}else{
		orderState = messages.get("order_field_moneyComplete_false");
	}
	orderState += " / ";
	if (orderHome.getInstance().isAllStoreOut()){
		orderState +=  messages.get("order_field_allStoreOut_true");
	}else{
		orderState +=  messages.get("order_field_allStoreOut_false");
	}

	orderState += " / ";
	if (orderHome.getInstance().isResReceived()){
	 	orderState += messages.get("order_field_resReceived_true"); 
	}else{ 
		orderState += messages.get("order_field_resReceived_false");
	}
	
	
	orderPayType = messages.get(orderHome.getInstance().getPayType().name());
	
	payItems = orderHome.getOrderPayOpers();]]></method>
