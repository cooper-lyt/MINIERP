            <property name="dataSource">Data Source</property>
            <method name="open"><![CDATA[importPackage(Packages.java.lang);
importPackage(Packages.org.jboss.seam);  
dictionary = Component.getInstance("dictionary", false);
condition= Component.getInstance("customerSearchCondition", true);
condition.setCustomerAreaId(params['customerAreaId'].value);
condition.setLevelFrom(params['customerLevelFrom'].value);
condition.setLevelTo(params['customerLevelTo'].value);
condition.setType(params['customerType'].value);
condition.setProvinceCode(params['customerName'].value);

customerList = Component.getInstance("customerList", true);
customerList.setMaxResults(null);

switch (params['orderBy'].value){
case 1:
	customerList.setOrderColumn('customer.balance');
	break;
case 2:
	customerList.setOrderColumn('orderArrears');
	break;
case 3:
	customerList.setOrderColumn('lastMoney');
	break;
case 4:
	customerList.setOrderColumn('orderTotalMoney');
	break;
case 5:
	customerList.setOrderColumn('orderCount');
	break;

}

it = customerList.getResultList().iterator();
]]></method>
            <method name="fetch"><![CDATA[if (it.hasNext()){
	item = it.next();
	row['Name'] = item.getName();
	row['Type'] = item.getType();
	row['Area'] = item.getArea();
	row['Level'] = item.getLevelName() + '(' + item.getLevel() + ')';
	row['City'] = dictionary.getCityName(item.getCityCode());
	row['Balance'] = item.getBalance();
	row['OrderArrears'] = item.getOrderArrears();
	row['LastMoney'] = item.getLastMoney();
	row['OrderTotalMoney'] = item.getOrderTotalMoney();
	row['OrderCount'] = item.getOrderCount();
	
	
	return true;
}
return false;	
	]]></method>
