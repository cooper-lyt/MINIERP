    <method name="initialize"><![CDATA[importPackage(Packages.java.lang);
importPackage(Packages.java.util);
importPackage(Packages.org.jboss.seam);  
importPackage(Packages.com.dgsoft.erp.model); 
importPackage(Packages.com.dgsoft.common); 
dictionary = Component.getInstance("dictionary", false);

dispatchHome = Component.getInstance("dispatchHome", false)
if ((dispatchHome != null) &amp;&amp; dispatchHome.isIdDefined()){

	orderHome = Component.getInstance("orderHome",true);
	order = dispatchHome.getInstance().getNeedRes().getCustomerOrder();
	//dispatchItems = dispatchHome.getInstance().getDispatchItemList();
	outDate = new Date();
	store = dispatchHome.getInstance().getStore().getName() + "(" + dispatchHome.getInstance().getStore().getId() + ")";
	shipAddres = dispatchHome.getInstance().getNeedRes().getAddress() +  "    " + dispatchHome.getInstance().getNeedRes().getPostCode();
	if (order.payType.equals(CustomerOrder.OrderPayType.EXPRESS_PROXY)){
		proxyMoney = orderHome.getShortageMoney(); 
	}else{
		proxyMoney = 0;
	}	
}else{

	orderHome = Component.getInstance("orderHome",true);
	orderHome.setId(params["orderId"].value);
	order = orderHome.getInstance();
	masterNeedRes = orderHome.getMasterNeedRes();
	//TODO NeedResID
	dispatchIt = masterNeedRes.getDispatchList().iterator();
	while(dispatchIt.hasNext()){
		dispatch = dispatchIt.next();
		if (dispatch.getStore().getId().equals(params["storeId"].value)){
			//dispatchItems = dispatch.getDispatchItemList();
			outDate = dispatch.getStockChange().operDate();
			store = dispatch.getStore().getName() + "(" + dispatch.getStore().getId() + ")";
			shipAddres = masterNeedRes.getAddress() + "   " + masterNeedRes.getPostCode() ;
			
			
		}	
	}

	
}

dispatchItems = Component.getInstance("orderStoreOutItems",true);


	salerName = dictionary.getEmpNameById(order.getOrderEmp());
	saleDate = order.getCreateDate();
	orderNumber = order.getId();
	customerName = order.getCustomer().getName();
	shipContact = order.getContact();
	shipContactTel = order.getTel();
]]></method>
