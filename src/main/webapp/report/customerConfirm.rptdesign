mIt = customerMoneyConfirm.getResultList().iterator();
]]></method>
            <method name="fetch"><![CDATA[if (mIt.hasNext()){
	accountOper = mIt.next();
	row['OperDate'] = accountOper.getOperDate();
	row['OperType'] = messages.get(accountOper.getOperType().name());
	row['Money'] = accountOper.getCustomerOperMoney();
	row['OperTypeOrdinal'] = accountOper.getOperType().ordinal();
	row['red'] = !accountOper.getOperType().getAdd();

	row['PayType'] = messages.get(accountOper.getMoneySave().getPayType().name());
	
	if (accountOper.getMoneySave().getPayType().equals(com.dgsoft.erp.model.api.PayType.BANK_TRANSFER) &amp;&amp; (accountOper.getMoneySave().getBankAccount() != null))
		row['bank'] = dictionary.getWordValue(accountOper.getMoneySave().getBankAccount().getBank());
	return true;
}else
	return false;]]></method>
