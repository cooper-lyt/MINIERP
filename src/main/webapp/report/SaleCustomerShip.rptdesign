]]></method>
            <method name="fetch"><![CDATA[if (rit == null){
	return false;
}

if (!rit.hasNext()){
	if (cit.hasNext()){
		curEntry = cit.next();
		rit = curEntry.getValue().entrySet().iterator();
		resEntry = rit.next();
	}else{
		return false;
	}
}else{
	resEntry = rit.next(); 	
}


row['customerName'] = curEntry.getKey().getCustomerArea().getName() + " - " + curEntry.getKey().getName();
row['customerId'] = curEntry.getKey().getId();
row['resName'] = resEntry.getKey().getRes().getName() + "(" + resEntry.getKey().getRes().getCode() + ")";
row['resID'] = resEntry.getKey().getRes().getId();
row['storeRes'] = resHelper.generateStoreResTitle(resEntry.getKey());

row['masterCount'] = resEntry.getValue().getMasterCount();
row['masterUnit'] = resEntry.getKey().getRes().getUnitGroup().getMasterUnit().getName();

if (resEntry.getKey().getRes().getUnitGroup().equals(UnitGroup.UnitGroupType.FLOAT_CONVERT)){
	row['auxCount'] = resEntry.getValue().getAuxCount();
	row['auxUnit'] = resEntry.getKey().getRes().getUnitGroup().getFloatAuxiliaryUnit();
}  




]]></method>
