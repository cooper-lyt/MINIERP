            <method name="open"><![CDATA[importPackage(Packages.java.lang);
importPackage(Packages.java.util);
importPackage(Packages.org.jboss.seam);  
importPackage(Packages.com.dgsoft.erp.model); 
importPackage(Packages.com.dgsoft.common); 
dictionary = Component.getInstance("dictionary", false);
resFormatCache = Component.getInstance("resFormatCache",false);
resHelper = Component.getInstance("resHelper",true);
messages = Component.getInstance("org.jboss.seam.international.messages", false);

searchDateArea = Component.getInstance("searchDateArea",true);
searchDateArea.setDateFromTime(params['DateFrom'].value);
searchDateArea.setDateToTime(params['DateTo'].value);

storeResFilter = Component.getInstance("storeResFilter",true);
storeResFilter.setResCategoryIdsStr(params['ResCategoryIds'].value);
storeResFilter.setStoreResIdsStr(params['StoreResIds'].value);
storeResFilter.setResId(params['ResId'].value);
storeResFilter.setFloatConvertRate(params['FloatConvertRate'].value);

storeChangeTotalReport = Component.getInstance("storeChangeTotalReport", true);
storeChangeTotalReport.setStoreId(params['StoreId'].value);
it = storeChangeTotalReport.getResultList().iterator();

]]></method>
            <method name="fetch"><![CDATA[if (it.hasNext()){
	item = it.next();
	row['RES'] = item.getStoreRes().getRes().getName();
	brandFormat = resFormatCache.getFormatByDefineName(item.getStoreRes().getId(),"品牌");
	if (brandFormat == null){
		row['BRAND'] = "";
	}else
	row['BRAND'] = dictionary.getWordValue(brandFormat.getFormatValue());

	row['FORMAT'] = resHelper.getFormatsTitle(resFormatCache.getOtherFormats(item.getStoreRes().getId(),"品牌"),false);
	row['DIR'] = item.getStockChange().getOperType().isOut() ? "出库" : "入库";
	row['TYPE'] = item.getStockChange().getOperType().equals(com.dgsoft.erp.model.StockChange.StoreChangeType.ALLOCATION_OUT) ? messages.get("ALLOCATION_OUT") + item.getStockChange().getAllocationForStoreOut().getInStore().getName() : messages.get(item.getStockChange().getOperType().name());
	if (item.getMasterCount() == null){
	row['MASTER_COUNT'] = 0;
	}else
	row['MASTER_COUNT'] = item.getMasterCount();
	row['MASTER_UNIT'] = item.getStoreRes().getRes().getUnitGroup().getMasterUnit().getName();
	if (item.getStoreRes().getRes().getUnitGroup().getType().equals(UnitGroup.UnitGroupType.FLOAT_CONVERT)){
		row['AUX_COUNT'] = item.getAuxCount();
		row['AUX_UNIT'] = item.getStoreRes().getRes().getUnitGroup().getFloatAuxiliaryUnit().getName();
	}else{
		row['AUX_COUNT'] = 0;
	}
return true;
}
return false;]]></method>
