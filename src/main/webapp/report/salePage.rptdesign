            <property name="dataSource">Data Source</property>
            <method name="open"><![CDATA[i=0;]]></method>
            <method name="fetch"><![CDATA[if (i < orderItems.size()){
	item = orderItems.get(i)
	if (item.isStoreResItem()){
		row['NAME'] = item.getStoreRes().getRes().getName();
	}else{
		row['NAME'] = item.getRes().getName();
	}
    row['COUNT'] = item.getCount();
    row['UNIT']= item.getUseUnit().getName();
    row['TOTAL_PRICE'] = item.getTotalPrice();
	i++;

	return true;
}else{
	return false;
}]]></method>
