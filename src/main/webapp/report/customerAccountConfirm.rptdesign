            <method name="open"><![CDATA[
sIt = customerResConfirmTotal.getResultList().iterator();]]></method>
            <method name="fetch"><![CDATA[if (sIt.hasNext()){
	item = sIt.next();
	row['STORE_RES'] = resHelper.generateStoreResTitle(item.getStoreRes(),true);
	if (item instanceof com.dgsoft.erp.model.OrderItem){
		row['OUT_DATE'] = item.getDispatch().getSendTime();
		row['GROUP_ID'] = item.getDispatch().getNeedRes().getCustomerOrder().getId();
		row['IS_SALE_GROUP'] = true;
		row['OPER_TYPE'] = '订单发货';
		row['GROUP_REBATE'] = item.getDispatch().getNeedRes().getCustomerOrder().getTotalRebateMoney();
		row['GROUP_MONEY'] = item.getDispatch().getNeedRes().getCustomerOrder().getMoney(); 
	}else{
		row['OUT_DATE'] = item.getDispatch().getStockChange().getOperDate();
		row['GROUP_ID'] = item.getDispatch().getOrderBack().getId();
		row['IS_SALE_GROUP'] = false;
		row['OPER_TYPE'] = '退货';
		row['GROUP_REBATE'] = 0;
		row['GROUP_MONEY'] = item.getDispatch().getOrderBack().getMoney();
	}
	row['UNIT_MONEY'] = item.getMoney();
	row['MONEY_UNIT'] = item.getResUnit().getName();
	row['MONEY_UNIT_COUNT'] = item.getUseUnitCount();
	row['MONEY'] = item.getTotalMoney();
	row['REBATE'] = item.getRebate();
	return true;
}else
	return false;]]></method>
