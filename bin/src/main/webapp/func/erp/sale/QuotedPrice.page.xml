<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd">

    <param name="firstResult" value="#{quotedPriceList.firstResult}"/>
    <param name="sort" value="#{quotedPriceList.orderColumn}"/>
    <param name="dir" value="#{quotedPriceList.orderDirection}"/>



    <param name="customerAreaId" value="#{quotedPriceList.customerAreaId}"/>

    <param name="customerName" value="#{quotedPriceList.customerName}"/>

    <param name="quotedType" value="#{quotedPriceList.type}"/>

    <param name="customerLevelFrom" value="#{quotedPriceList.levelFrom}" />
    <param name="customerLevelTo" value="#{quotedPriceList.levelTo}" />
    <param name="quotedEmpId" value="#{quotedPriceList.empId}" />

    <param name="createDateFrom" value="#{quotedPriceList.searchDateArea.dateFrom}"  converterId="searchDateConverter"/>
    <param name="createDateTo" value="#{quotedPriceList.searchDateArea.dateTo}"  converterId="searchDateConverter"/>



</page>