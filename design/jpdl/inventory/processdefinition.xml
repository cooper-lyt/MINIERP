<?xml version="1.0" encoding="UTF-8"?>

<process-definition  xmlns=""  name="inventory">


	<start-state name="开始盘点">
		<transition to="盘点"></transition>
	</start-state>


	<task-node name="盘点">
		<task name="盘点">
			<description>
				{&quot;description&quot;:&quot;&quot;,&quot;operPage&quot;:&quot;/business/taskOperator/erp/store/InventoryFirst.xhtml&quot;}
			</description>
			<assignment actor-id="#{actor.id}"></assignment>
		</task>
		<transition to="盘点审核"></transition>
	</task-node>

	<task-node name="盘点审核">
		<transition to="decision1"></transition>
	</task-node>

	<decision name="decision1">
		<transition to="盘点结束"></transition>
		<transition to="盘点" name="to 盘点"></transition>
	</decision>


	<end-state name="盘点结束"></end-state>


</process-definition>